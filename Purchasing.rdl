  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ProductID">
            <Value>=Parameters!ProductID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Production.Product.ProductID, Production.Product.Name AS ProdName, Purchasing.PurchaseOrderDetail.UnitPrice, Purchasing.PurchaseOrderDetail.ReceivedQty, Purchasing.PurchaseOrderHeader.RevisionNumber, 
                         Purchasing.PurchaseOrderHeader.ShipDate, Purchasing.Vendor.Name AS VendName
FROM            Production.Product INNER JOIN
                         Purchasing.PurchaseOrderDetail ON Production.Product.ProductID = Purchasing.PurchaseOrderDetail.ProductID INNER JOIN
                         Purchasing.PurchaseOrderHeader ON Purchasing.PurchaseOrderDetail.PurchaseOrderID = Purchasing.PurchaseOrderHeader.PurchaseOrderID INNER JOIN
                         Purchasing.Vendor ON Purchasing.PurchaseOrderHeader.VendorID = Purchasing.Vendor.BusinessEntityID
WHERE Production.Product.ProductID=@ProductID</CommandText>
      </Query>
      <Fields>
